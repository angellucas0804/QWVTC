<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.gob.qw.vigilatucole">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="pe.gob.qw.vigilatucole.application.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="pe.gob.qw.vigilatucole.EncuestaActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />

        <activity
            android:name="pe.gob.qw.vigilatucole.AgregarEditarPerfilActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />

        <activity
            android:name="pe.gob.qw.vigilatucole.PerfilActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />

        <activity
            android:name="pe.gob.qw.vigilatucole.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>